Cell["I have a function called PackageGenerator:", "Text",
 CellChangeTimes->{{3.727995588058421*^9, 
  3.727995599094654*^9}},ExpressionUUID->"bed94d06-3667-453c-acb1-\
fac85166322e"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"PackageGenerator", "[", "theName_String", "]"}], ":=", "\n", 
   RowBox[{"Catch", "@", 
    RowBox[{"Module", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"name", ",", "document", ",", "directory", ",", "nb"}], "}"}], 
      ",", "\n", "\n", 
      RowBox[{
       RowBox[{"name", "=", 
        RowBox[{"StringReplace", "[", 
         RowBox[{"theName", ",", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{"Except", "[", "WordCharacter", "]"}], "->", "\"\<\>\""}],
            "}"}]}], "]"}]}], ";", "\n", "\n", 
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{"name", "===", "\"\<\>\""}], ",", 
         RowBox[{"Throw", "[", "$Failed", "]"}]}], "]"}], ";", "\n", "\n", 
       RowBox[{"document", "=", "\n", 
        RowBox[{"DocumentNotebook", "[", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"{", "\n", 
           RowBox[{
            RowBox[{
             RowBox[{
              RowBox[{"TextCell", "[", 
               RowBox[{"#", ",", "\"\<Title\>\"", ",", 
                RowBox[{"TextAlignment", "->", "Center"}]}], "]"}], "&"}], 
             "[", 
             RowBox[{"name", "<>", "\"\<.m package\>\""}], "]"}], ",", "\n", 
            RowBox[{
             RowBox[{
              RowBox[{"ExpressionCell", "[", 
               RowBox[{
                RowBox[{"Defer", "[", 
                 RowBox[{"BeginPackage", "[", "#", "]"}], "]"}], ",", 
                "\"\<Input\>\""}], "]"}], "&"}], "[", 
             RowBox[{"name", "<>", "\"\<`\>\""}], "]"}], ",", "\n", 
            RowBox[{"TextCell", "[", 
             RowBox[{"\"\<Usage messages\>\"", ",", "\"\<Subsection\>\""}], 
             "]"}], ",", "\n", 
            RowBox[{"TextCell", "[", 
             RowBox[{"\"\<`Private context\>\"", ",", "\"\<Section\>\""}], 
             "]"}], ",", "\n", 
            RowBox[{"ExpressionCell", "[", 
             RowBox[{
              RowBox[{"Defer", "[", 
               RowBox[{"Begin", "[", "\"\<`Private`\>\"", "]"}], "]"}], ",", 
              "\"\<Input\>\""}], "]"}], ",", "\n", 
            RowBox[{"TextCell", "[", 
             RowBox[{"\"\<Definitions\>\"", ",", "\"\<Subsection\>\""}], 
             "]"}], ",", "\n", 
            RowBox[{"TextCell", "[", 
             RowBox[{
             "\"\<End of `Private context\>\"", ",", "\"\<Section\>\""}], 
             "]"}], ",", "\n", 
            RowBox[{"ExpressionCell", "[", 
             RowBox[{
              RowBox[{"Defer", "[", 
               RowBox[{"End", "[", "]"}], "]"}], ",", "\"\<Input\>\""}], 
             "]"}], ",", "\n", 
            RowBox[{"TextCell", "[", 
             RowBox[{
             "\"\<End of package context\>\"", ",", "\"\<Section\>\""}], 
             "]"}], ",", "\n", 
            RowBox[{"ExpressionCell", "[", 
             RowBox[{
              RowBox[{"Defer", "[", 
               RowBox[{"EndPackage", "[", "]"}], "]"}], ",", 
              "\"\<Input\>\""}], "]"}]}], "\n", "}"}], "\[IndentingNewLine]", 
          ",", "\n", 
          RowBox[{"StyleDefinitions", "->", 
           RowBox[{"Notebook", "[", 
            RowBox[{
             RowBox[{"{", "\n", 
              RowBox[{
               RowBox[{"Cell", "[", 
                RowBox[{"StyleData", "[", 
                 RowBox[{"StyleDefinitions", "->", "\"\<Default.nb\>\""}], 
                 "]"}], "]"}], ",", 
               RowBox[{"Cell", "[", 
                RowBox[{
                 RowBox[{"StyleData", "[", "\"\<Code\>\"", "]"}], ",", 
                 RowBox[{"PageWidth", "->", "WindowWidth"}], ",", 
                 RowBox[{"AutoIndent", "->", "Automatic"}], ",", 
                 RowBox[{"AutoSpacing", "->", "True"}], ",", 
                 RowBox[{"LineBreakWithin", "->", "Automatic"}], ",", 
                 RowBox[{"LineIndent", "->", "1"}], ",", 
                 RowBox[{"LinebreakAdjustments", "->", 
                  RowBox[{"{", 
                   RowBox[{"0.85", ",", "2", ",", "10", ",", "0", ",", "1"}], 
                   "}"}]}]}], "]"}], ",", 
               RowBox[{"Cell", "[", 
                RowBox[{
                 RowBox[{"StyleData", "[", "\"\<Input\>\"", "]"}], ",", 
                 RowBox[{"InitializationCell", "->", "True"}]}], "]"}]}], 
              "}"}], ",", 
             RowBox[{"Visible", "->", "False"}], ",", 
             RowBox[{
             "StyleDefinitions", "->", 
              "\"\<PrivateStylesheetFormatting.nb\>\""}]}], "]"}]}], ",", 
          "\n", 
          RowBox[{"AutoGeneratedPackage", "->", "Automatic"}]}], "\n", 
         "]"}]}], ";", "\n", "\n", 
       RowBox[{"nb", "=", 
        RowBox[{"CreateWindow", "[", "document", "]"}]}], ";", "\n", "\n", 
       RowBox[{"directory", "=", 
        RowBox[{"SystemDialogInput", "[", 
         RowBox[{"\"\<Directory\>\"", ",", " ", "\n", 
          RowBox[{"FileNameJoin", "[", 
           RowBox[{"{", 
            RowBox[{"$UserBaseDirectory", ",", "\"\</Applications\>\""}], 
            "}"}], "]"}]}], "]"}]}], ";", "\n", "\n", 
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{"directory", "===", "$Canceled"}], ",", 
         RowBox[{"Throw", "[", "$Canceled", "]"}]}], "]"}], ";", "\n", "\n", 
       RowBox[{"NotebookSave", "[", 
        RowBox[{"nb", ",", 
         RowBox[{"FileNameJoin", "[", 
          RowBox[{"{", 
           RowBox[{"directory", ",", 
            RowBox[{"name", "<>", "\"\<.nb\>\""}]}], "}"}], "]"}]}], 
        "]"}]}]}], "\n", "]"}]}]}], ";"}]], "Input",
 CellOpen->False,ExpressionUUID->"4a67c9e7-288b-471c-8520-bcb31782fb8e"],

