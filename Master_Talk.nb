Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"PushOptions", "[", 
    RowBox[{"fn_", ",", "explicitOptions_", ",", "parentSymbol_"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"opts1", ",", "opts2"}], "}"}], ",", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"opts1", "=", 
       RowBox[{"FilterRules", "[", 
        RowBox[{
         RowBox[{"Options", "[", "parentSymbol", "]"}], ",", 
         RowBox[{"Except", "[", "explicitOptions", "]"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"opts2", "=", 
       RowBox[{"FilterRules", "[", 
        RowBox[{
         RowBox[{"Flatten", "[", 
          RowBox[{"{", 
           RowBox[{"opts1", ",", "explicitOptions"}], "}"}], "]"}], ",", 
         RowBox[{"Options", "[", "fn", "]"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"fn", "[", 
        RowBox[{"##", ",", 
         RowBox[{"Evaluate", "[", "opts2", "]"}]}], "]"}], "&"}]}]}], 
    "\[IndentingNewLine]", "]"}]}], ";"}]], "Input",
 CellChangeTimes->{{3.7279857916145773`*^9, 3.727985795123953*^9}, 
   3.7279859542770576`*^9, {3.727986086201651*^9, 3.727986092033534*^9}, {
